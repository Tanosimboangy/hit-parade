wSong = state.map((song) => {\n\t\t\t\tif(song.id === action.payload) {\n\t\t\t\t\treturn {\n\t\t\t\t\t\t...song,\n\t\t\t\t\t\tdislike: song.dislike + 1,\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\treturn song;\n\t\t\t})\n\t\t\treturn newSong;\n\t\t}\n\t\tdefault:\n\t\t\treturn state;\n\t}\n}\n\n\nfunction cartItems(state = [], action) {\n\tswitch (action.type) {\n\t\tcase 'ADD_TO_CART': {\n\t\t\treturn [...state, action.payload];\n\t\t}\n\t\tcase 'REMOVE_CART_ITEM':\n\t\t\tconst newCartWithoutSpecificItem = state.filter(\n\t\t\t\t(item) => item.id !== action.payload\n\t\t\t);\n\t\t\treturn [...newCartWithoutSpecificItem];\n\t\tcase 'EMPTY_CART':\n\t\t\treturn [];\n\t\tdefault:\n\t\t\treturn state;\n\t}\n}\n\n\nexport default combineReducers({\n\tsongs,\n\tcartItems,\n});\n"},"lineCount":null}},"error":null,"hash":"968af445290be4f907b688519bc37746","cacheData":{"env":{}}}